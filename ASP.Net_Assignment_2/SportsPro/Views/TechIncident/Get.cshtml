@model IncidentViewModel

@{
    
    ViewData["Title"] = "Get Technician";
}

<h1>Technician</h1>


<form asp-action="List" method="get">

    <div class="form-group">
        <label asp-for="@Model.TechnicianID">Technician</label>
        <select asp-for="@Model.TechnicianID"
                asp-items="@(new SelectList( Model.Technicians,
                         "TechnicianID", "Name", 0))"
                class="form-control">
            <option value="">Assign a Technician...</option>
        </select>
    </div>
    <button type="submit" class="btn btn-info">Select</button>
</form>
